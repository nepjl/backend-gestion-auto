generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"] // ou d'autres cibles selon votre environnement
 
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cars {
  id        Int     @id @default(autoincrement())
  img_url     String
  mark   String?
  model  String?
  year    Int?
  color    String?
  mileage  Int?
  
  
}


// Mod√®le pour l'User
model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
 
}

